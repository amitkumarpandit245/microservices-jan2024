package com.bvr;


@RestController
public class MyEndPoints3 {

	public MyEndPoints3() {
		// TODO Auto-generated constructor stub
	}
	
	@Autowired
	ExampleProperties props;
	
	@RequestMapping("/greetOthers")
	public String greet(@RequestParam String details) {
		return props.getGreeting() + details;
	}

}
